<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:s="clr-namespace:System;assembly=mscorlib"
    xmlns:c="clr-namespace:elaphureLink.Wpf.Converters"
>

    <s:Boolean x:Key="True">True</s:Boolean>
    <s:Boolean x:Key="False">False</s:Boolean>

    <c:MultiBooleanToVisibilityConverter x:Key="MultiBooleanToVisibilityConverter"/>
    <c:BooleanInverseConverter x:Key="BooleanInverseConverter" />
    <c:BooleanToStatusColorConverter x:Key="BooleanToStatusColorConverter" />
    <c:BooleanToStringConverter x:Key="BooleanToStringConverter" />
    <c:ItemCountConverter x:Key="ItemCountConverter" />


</ResourceDictionary>
